# {{ ansible_managed }}

server {
    listen 80 default_server;
    listen [::]:80 default_server ipv6only=on;

    server_name _;

    location / {
        return 410;
    }

    location = /nginx_stub_status {
        allow 127.0.0.1;
        allow ::1;
        deny all;

        stub_status on;
    }

    include snippets/le.conf;
}
